// Gimlet Sequencer API

Interface(
    name: "Sequencer",
    ops: {
        "get_state": (
            doc: "Return the power state",
            reply: Result(
                ok: (
                    type: "PowerState",
                    recv: FromPrimitive("u8"),
                ),
                err: CLike("SeqError"),
            ),
        ),
        "set_state": (
            doc: "Set the power state",
            args: {
                "state": (
                    type: "PowerState",
                    recv: FromPrimitive("u8"),
                )
            },
            reply: Result(
                ok: "()",
                err: CLike("SeqError"),
            ),
        ),
        "fans_on": (
            args: {},
            reply: Result(
                ok: "()",
                err: CLike("SeqError"),
            ),
        ),
        "fans_off": (
            args: {},
            reply: Result(
                ok: "()",
                err: CLike("SeqError"),
            ),
        ),
        "read_idt_reg": (
            args: {
                "reg": "u8",
            },
            reply: Result(
                ok: "u8",
                err: CLike("SeqError"),
            ),
        ),
        "set_idt_page": (
            args: {
                "page": "u8",
            },
            reply: Result(
                ok: "()",
                err: CLike("SeqError"),
            ),
        ),
        "write_idt_reg": (
            args: {
                "reg": "u8",
                "value": "u8",
            },
            reply: Result(
                ok: "()",
                err: CLike("SeqError"),
            ),
        ),
    },
)
